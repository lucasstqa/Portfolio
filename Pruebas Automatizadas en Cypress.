       it('funcionalidad de boton y Pestaña de "Contact" ' , () => {

       cy.get(':nth-child(2) > .nav-link').click()
       cy.get('#recipient-email').type('Ejemplo')
       cy.get('#recipient-name').type('Ejemplo')
       cy.get('#message-text').type('Ejemplo')
       cy.get('#exampleModal > .modal-dialog > .modal-content > .modal-footer > .btn-primary').click()

       })


       it('Funcionalidad del boton y pestaña "about us" ', () => {

       cy.get(':nth-child(3) > .nav-link').click()
       cy.get('.vjs-poster').should('be.visible')

       })

       it('Prueba end to end' , () => {

       cy.get(':nth-child(1) > .card > :nth-child(1) > .card-img-top').click()
       cy.get('.col-sm-12 > .btn').click()
       cy.get('#nava').click()
       cy.get('#cartur').click()
       cy.get('#nava').click()
       cy.get(':nth-child(2) > .card > .card-block > .card-title > .hrefch').click()
       cy.get('.col-sm-12 > .btn').click()
       cy.get('#nava').click()
       cy.get('#cartur').click()
       cy.get('.col-lg-1 > .btn').click()
       cy.get('#name').type('Juan')
       cy.get('#country').type('Argentina')
       cy.get('#city').type('Buenos Aires')
       cy.get('#card').type('123456')
       cy.get('#month').type('febrero')
       cy.get('#year').type('2023')
       cy.get('#orderModal > .modal-dialog > .modal-content > .modal-footer > .btn-primary').click()
       cy.get('.sweet-alert').should('be.visible')
       cy.get('.confirm').click()
       cy.get('#orderModal > .modal-dialog > .modal-content > .modal-footer > .btn-primary').click()
       cy.get('.confirm').click()

       })


     })
